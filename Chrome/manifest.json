{
    "manifest_version": 3,
    "name": "Panopto Caption Extractor",
    "version": "1.0",
    "description": "Extracts captions from Panopto and sends them to a locally hosted web app.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "http://127.0.0.1:8080/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  